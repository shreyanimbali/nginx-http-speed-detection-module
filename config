ngx_addon_name=ngx_http_speed_header_module

if test -n "$ngx_module_link"; then
    # The New Way
    ngx_module_type=HTTP
    ngx_module_name=ngx_http_speed_header_module
    ngx_module_srcs="$ngx_addon_dir/ngx_http_speed_header_module.c"

    . auto/module
else
    # The Old Way
    HTTP_MODULES="$HTTP_MODULES ngx_http_speed_header_module"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_http_speed_header_module.c"
fi
